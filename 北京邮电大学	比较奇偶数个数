#include <iostream>
using namespace std;

int main()
{
    int n;
    while (cin >> n) {
        int i, tmp, evencnt = 0, oddcnt = 0;
        for (i=0; i<n; ++i) {
            cin >> tmp;
            if (tmp%2 == 0) evencnt++;
            else oddcnt++;
        }
        if (evencnt > oddcnt) cout << "NO" << endl;
        else cout << "YES" << endl;
    }
    return 0;
}
