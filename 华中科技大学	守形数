#include <iostream>
using namespace std;

int main()
{
    int N;
    while (cin >> N) {
        int N2 = N * N;
        if (N > 9) {
            if ((N2-N)%100 == 0) cout << "Yes!" << endl;
            else cout << "No!" << endl;
        }
        else {
            if ((N2-N)%10 == 0) cout << "Yes!" << endl;
            else cout << "No!" << endl;
        }
    }
    return 0;
}
