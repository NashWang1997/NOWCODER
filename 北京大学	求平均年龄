#include <iostream>
#include <cstdlib>
using namespace std;

int main()
{
    int n;
    while (cin >> n) {
        int i, age, cnt = 0;
        for (i=0; i<n; ++i) {
            cin >> age;
            cnt += age;
        }
        printf("%.2f", cnt*1.0/n);
        printf("\n");
    }
    return 0;
}
