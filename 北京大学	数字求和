#include <iostream>
using namespace std;

int main()
{
    int a;
    while (cin >> a) {
        int i, input, cnt = 0;
        for (i=0; i<5; ++i) {
            cin >> input;
            if (input < a) cnt += input;
        }
        cout << cnt << endl;
    }
    return 0;
}
