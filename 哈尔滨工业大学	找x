#include <iostream>
using namespace std;

int main()
{
    int n;
    while (cin >> n) {
        int data[n];
        int i, x, p = -1;
        for (i=0; i<n; ++i) cin >> data[i];
        cin >> x;
        for (i=0; i<n; ++i)
            if (x == data[i]) {
                p = i;
                break;
            }
        cout << p << endl;
    }
    return 0;
}
