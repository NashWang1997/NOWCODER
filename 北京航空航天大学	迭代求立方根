#include <iostream>
using namespace std;

int main()
{
    int n;
    while (cin >> n) {
        int i, input, even = 0;
        for (i=0; i<n; ++i) {
            cin >> input;
            if (input%2 == 0) even++;
        }
        if (even > n-even) cout << "NO" << endl;
        else cout << "YES" << endl;
    }
    return 0;
}
