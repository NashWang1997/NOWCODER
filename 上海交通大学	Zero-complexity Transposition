#include <iostream>
#include <string>
using namespace std;

int main()
{
    int n;
    while (cin >> n) {
        string numbers[n];
        int i;
        for (i=0; i<n; ++i) cin >> numbers[i];
        for (i=n-1; i>=0; --i) cout << numbers[i] << ' ';
        cout << endl;
    }
    return 0;
}
