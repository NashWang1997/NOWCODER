#include <iostream>
using namespace std;

int main()
{
    int input, M, i;
    while (cin >> input) {
        M = input;
        for (i=0; i<9; ++i) {
            cin >> input;
            if (input > M) M = input;
        }
        cout << "max=" << M << endl;
    }
    return 0;
}
