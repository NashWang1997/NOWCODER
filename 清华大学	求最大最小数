#include <iostream>
using namespace std;

int main()
{
    int N;
    while (cin >> N) {
        int m = 1000000, M = -1000000, i, tmp;
        for (i=0; i<N; ++i) {
            cin >> tmp;
            if (tmp < m) m = tmp;
            if (tmp > M) M = tmp;
        }
        cout << M << ' ' << m << endl;
    }
    return 0;
}
